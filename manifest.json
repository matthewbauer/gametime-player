{
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "manifest_version": 2,
  "name": "GPemu",
  "version": "1.0.1",
  "description": "A video game emulator for Chrome supporting NES, SNES, Gameboy, and Gameboy Advance ROMs.",
  "icons": {
    "512": "icon/512.png",
    "128": "icon/128.png"
  },
  "author": {
    "name": "Matthew Bauer",
    "url": "https://matthewbauer.us"
  },
  "file_handlers": {
    "snes9x-next": {
      "extensions": [
        "sfc",
        "smc"
      ]
    },
    "vba-next": {
      "extensions": [
        "gba"
      ]
    },
    "gambatte": {
      "extensions": [
        "gb"
      ]
    },
    "nestopia": {
      "extensions": [
        "nes"
      ]
    }
  },
  "offline_enabled": true,
  "requirements": {
    "3D": {
      "features": ["webgl"]
    }
  },
  "permissions": [
    "syncFileSystem",
    "storage"
  ]
}
